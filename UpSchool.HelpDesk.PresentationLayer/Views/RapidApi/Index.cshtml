@model CreateWorkRequestModel

@{
    Layout = "_Layout";
}

<div class="card">
    <div class="card-body">
        <h3 class="card-title mb-3">Rapid Api Covid Sorgusu (Ülke ismini ingilizce yazınız)</h3>
        <p>Örn : China,Mauritania,Gambia,England, Greenland </p>
        <form asp-action="List" asp-controller="RapidApi" method="get">

            <div class="form-group">
                <label for="example-text-input" class="form-control-label">Ülke</label>
                <input class="form-control" type="text" name="country">
            </div>
           

            <div class="form-group">
                <button type="submit" class="btn btn-primary btn-lg w-100">Sorgula</button>
            </div>
        </form>

    </div>

</div>

